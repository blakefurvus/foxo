# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := foxo

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

foxo: 
	@echo "==== Building foxo ($(config)) ===="
	@${MAKE} --no-print-directory -C foxo -f Makefile

clean:
	@${MAKE} --no-print-directory -C foxo -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   foxo"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
